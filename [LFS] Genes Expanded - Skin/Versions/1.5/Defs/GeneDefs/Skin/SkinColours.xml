    <GeneDef ParentName="GeneSkinColorOverride">
        <defName>GES_Skin_Yellowfin</defName>
        <displayCategory>GES_SkinColours</displayCategory>
        <label>Yellowfin</label>
        <labelShortAdj>Yellow</labelShortAdj>
        <description>Carriers of this gene produce a pigment that gives their skin a yellow color.</description>
        <randomBrightnessFactor>0.2</randomBrightnessFactor>
        <skinColorOverride>(243, 240, 143)</skinColorOverride>
        <iconPath>UI/Icon/Genes/SkinColor_Icon</iconPath>

        <displayOrderInCategory>0</displayOrderInCategory>
        <symbolPack>
            <prefixSymbols>
                <li>
                    <symbol>Skin</symbol>
                </li>
            </prefixSymbols>
        </symbolPack>
    </GeneDef>

